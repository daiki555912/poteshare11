<h1>Bookings#new</h1>
<p>Find me in app/views/bookings/new.html.erb</p>

<%= form_for @booking do |f| %>
  <%= f.label :start_date, "開始日" %>
  <%= f.date_field :start_date %>

  <%= f.label :last_date, "終了日" %>
  <%= f.date_field :last_date %>

  <%= f.label :people, "人数" %>
  <%= f.number_field :people %>

  <%= f.submit "予約する" %>
<% end %>
